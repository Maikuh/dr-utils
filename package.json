{
  "name": "dr-utils",
  "version": "2.0.0-beta.11",
  "description": "Set of utilities relevant to the Dominican Republic",
  "author": "Miguel Araujo",
  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*",
    "LICENSE",
    "README.md"
  ],
  "type": "module",
  "scripts": {
    "generate-rnc-cedulas": "bun run ./scripts/generate-rnc-cedulas.ts",
    "generate-provincias-municipios": "bun run ./scripts/generate-provincias-municipios.ts",
    "preversion": "bun run lint",
    "prepublishOnly": "bun run lint && bun run build",
    "postversion": "git push --follow-tags",
    "prebuild": "rimraf lib && tsc --noEmit -p tsconfig.build.json",
    "build": "tsdown",
    "format": "biome format --write",
    "lint": "biome lint --write",
    "biome": "biome check --write",
    "biome:ci": "biome ci",
    "test:coverage": "bun test --coverage"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.10",
    "@types/bun": "^1.3.5",
    "extract-zip": "^2.0.1",
    "rimraf": "^6.1.2",
    "tsdown": "^0.18.4",
    "typescript": "^5.9.3"
  },
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "types": "./lib/index.d.ts"
    },
    "./types/*": "./lib/types/*",
    "./utils/*": "./lib/utils/*",
    "./constants/*": "./lib/constants/*",
    "./formatters/*": "./lib/formatters/*",
    "./validators/*": "./lib/validators/*",
    "./package.json": "./package.json"
  },
  "keywords": [
    "dominican republic",
    "republica dominicana",
    "dr",
    "rd",
    "rep dom",
    "dominicano",
    "dominicana",
    "identification",
    "cedula",
    "rnc",
    "registro nacional de contribuyentes",
    "ncf",
    "numero de comprobante fiscal",
    "phone number",
    "telefono",
    "numero telefonico",
    "validation",
    "validacion",
    "validator",
    "validador",
    "formatter",
    "formateador",
    "util",
    "utils",
    "utilidad",
    "utilidades",
    "helpers"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Maikuh/dr-utils.git"
  }
}
